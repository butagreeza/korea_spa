<odoo>
    <data>
        <record id="partner_profile_image_tree_view" model="ir.ui.view">
            <field name="name">Partner profile</field>
            <field name="model">partner.profile.image</field>
            <field name="arch" type="xml">
                <tree default_order="create_date desc">
                    <field name="note"/>
                    <field name="create_date"/>
                    <field name="create_uid"/>
                    <field name="write_date"/>
                    <field name="write_uid"/>
                </tree>
            </field>
        </record>
        <record id="partner_profile_image_form_view" model="ir.ui.view">
            <field name="name">Partner profile</field>
            <field name="model">partner.profile.image</field>
            <field name="arch" type="xml">
                <form>
                    <field name="partner_id" invisible="True"/>
                    <label for="note"/>
                    <field name="note"/>
                    <field name="image_large" widget="image"/>
                </form>
            </field>
        </record>
    </data>
</odoo>