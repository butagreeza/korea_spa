<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record model="ir.cron" id="get_voip_phonecall">
            <field name="name">Get voip_phonecall</field>
            <field name="model_id" ref="model_phonecall"/>
            <field name="state">code</field>
            <field name="code">model.get_recoding_file()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="True" />
        </record>

        <record model="ir.cron" id="synchronize_partner">
            <field name="name">Synchronize Phone Partner</field>
            <field name="model_id" ref="model_phonecall"/>
            <field name="state">code</field>
            <field name="code">model.partner_synchronize()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="True" />
        </record>
    </data>
</odoo>