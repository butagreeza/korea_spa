<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="report_partner_feedback_form" model="ir.ui.view">
            <field name="name">Report partner feedback</field>
            <field name="model">report.partner.feedback</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <group>
                            <field name="from_date" required="1"/>
                            <field name="to_date" required="1"/>
                        </group>

                    </group>
                    <footer>
                        <button class="oe_highlight" name="create_report_partner_feedback" type="object" string="Print Report"/>
                        <button class="oe_highlight" name="create_report_partner_feedback_excel" type="object" string="Print Report Excel"/>
                    </footer>
                </form>
            </field>
        </record>


        <record id="report_partner_feedback_action" model="ir.actions.act_window">
            <field name="name">Report partner feedback</field>
            <field name="res_model">report.partner.feedback</field>
            <field name="view_mode">form</field>
            <field name="view_type">form</field>
            <field name="target">new</field>
        </record>
        <!--<menuitem id="pos_report" name="Pos Report" parent="point_of_sale.menu_point_root" sequence="11"/>-->
        <menuitem id="report_partner_feedback_menu" action="report_partner_feedback_action" name="Report partner feedback" parent="crm.crm_menu_report"/>
    </data>
</odoo>