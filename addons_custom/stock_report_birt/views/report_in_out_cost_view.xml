<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="report_in_out_cost_form" model="ir.ui.view">
            <field name="name">Report In Out Cost</field>
            <field name="model">report.in.out.cost</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <group>
                            <field name="from_date" required="1"/>
                            <field name="to_date" required="1"/>
                        </group>
                        <group>
                            <field name="location_id" domain="[('usage', '=', 'internal')]" options="{'no_create': True, 'no_create_edit':True}" required="1"/>
                            <field name="product_ids" options="{'no_create':1, 'no_create_edit':1}" widget="many2many_tags"/>
                            <field name="user_ids" options="{'no_create':1, 'no_create_edit':1}" widget="many2many_tags"/>
                        </group>
                    </group>
                    <footer>
                        <button class="oe_highlight" name="create_report_in_out_cost" type="object" string="Print Report"/>
                        <button class="oe_highlight" name="create_report_in_out_cost_excel" type="object" string="Print Report Excel"/>
                    </footer>
                </form>
            </field>
        </record>
        <record id="report_in_out_cost_action" model="ir.actions.act_window">
            <field name="name">Report In Out Cost</field>
            <field name="res_model">report.in.out.cost</field>
            <field name="view_mode">form</field>
            <field name="view_type">form</field>
            <field name="target">new</field>
        </record>
        <!--<menuitem id="pos_report" name="Pos Report" parent="point_of_sale.menu_point_root" sequence="11"/>-->
        <menuitem id="report_in_out_cost_menu" action="report_in_out_cost_action" name="Report In Out Cost" parent="stock.menu_warehouse_report"/>
    </data>
</odoo>