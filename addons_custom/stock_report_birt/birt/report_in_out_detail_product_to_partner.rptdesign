            <xml-property name="queryText"><![CDATA[SELECT
chi_tiet_xuat_nhap_hang_theo_khach_hang.*
FROM (
	(--Đơn hàng
	SELECT 
	d.x_old_code as ma_kh
	,d.name as ten_kh
	,g.default_code as ma_sp
	,g.name as ten_sp
	, h.name as don_vi
	, e.product_uom_qty as so_luong
	, c.name as ma_hoa_don
	, c.date_done + interval '7 hour' as ngay_hoa_don
	, 'Đơn hàng' as nguon
	, a.name as chi_tiet_nguon
	, CASE WHEN i.code = 'outgoing' THEN 'Xuất' WHEN i.code = 'incoming' THEN 'Nhập' ELSE i.code END as loai
	FROM pos_order a 
	LEFT JOIN pos_order_stock_picking_rel b ON a.id = b.pos_order_id
	LEFT JOIN stock_picking c ON b.stock_picking_id = c.id
	LEFT JOIN res_partner d ON a.partner_id = d.id
	LEFT JOIN stock_move e ON c.id = e.picking_id
	LEFT JOIN product_product f ON e.product_id = f.id
	LEFT JOIN product_template g ON f.product_tmpl_id = g.id
	LEFT JOIN product_uom h ON h.id = g.uom_id
	LEFT JOIN stock_picking_type i ON c.picking_type_id = i.id
	WHERE (c.date_done + interval '7 hour')::DATE >= ?::DATE
	AND (c.date_done + interval '7 hour')::DATE <= ?::DATE
	AND c.state = 'done'
	AND (d.id = ANY(string_to_array( ?, ',')::integer[]) or ? = '0')
	AND (c.id = ANY(string_to_array( ?, ',')::integer[]) or ? = '0')
	AND (f.id = ANY(string_to_array( ?, ',')::integer[]) or ? = '0')
	)
	
	UNION ALL

	(--Phiếu chỉ định
	SELECT 
	c.x_old_code as ma_kh
	,c.name as ten_kh
	,f.default_code as ma_sp
	,f.name as ten_sp
	, g.name as don_vi
	, d.product_uom_qty as so_luong
	, b.name as ma_hoa_don
	, b.date_done + interval '7 hour' as ngay_hoa_don
	, 'Phiếu chỉ định' as nguon
	, a.name as chi_tiet_nguon
	, CASE WHEN h.code = 'outgoing' THEN 'Xuất' WHEN h.code = 'incoming' THEN 'Nhập' ELSE h.code END as loai
	FROM therapy_prescription a 
	LEFT JOIN stock_picking b ON a.id = b.x_therapy_prescription_id
	LEFT JOIN res_partner c ON b.partner_id = c.id 
	LEFT JOIN stock_move d ON b.id = d.picking_id
	LEFT JOIN product_product e ON d.product_id = e.id
	LEFT JOIN product_template f ON e.product_tmpl_id = f.id
	LEFT JOIN product_uom g ON g.id = f.uom_id
	LEFT JOIN stock_picking_type h ON b.picking_type_id = h.id
	WHERE (b.date_done + interval '7 hour')::DATE >= ?::DATE
	AND (b.date_done + interval '7 hour')::DATE <= ?::DATE
	AND b.state = 'done'
	AND (c.id = ANY(string_to_array( ?, ',')::integer[]) or ? = '0')
	AND (b.id = ANY(string_to_array( ?, ',')::integer[]) or ? = '0')
	AND (e.id = ANY(string_to_array( ?, ',')::integer[]) or ? = '0')
	)

	UNION ALL

	(-- Nguyên vật liệu
	SELECT 
	c.x_old_code as ma_kh
	,c.name as ten_kh
	,f.default_code as ma_sp
	,f.name as ten_sp
	, g.name as don_vi
	, d.product_uom_qty as so_luong
	, b.name as ma_hoa_don
	, b.date_done + interval '7 hour' as ngay_hoa_don
	, 'Nguyên vật liệu' as nguon
	, a.name as chi_tiet_nguon
	, CASE WHEN h.code = 'outgoing' THEN 'Xuất' WHEN h.code = 'incoming' THEN 'Nhập' ELSE h.code END as loai
	FROM pos_user_material a
	LEFT JOIN stock_picking b ON a.picking_id = b.id
	LEFT JOIN res_partner c ON b.partner_id = c.id 
	LEFT JOIN stock_move d ON b.id = d.picking_id
	LEFT JOIN product_product e ON d.product_id = e.id
	LEFT JOIN product_template f ON e.product_tmpl_id = f.id
	LEFT JOIN product_uom g ON g.id = f.uom_id
	LEFT JOIN stock_picking_type h ON b.picking_type_id = h.id
	WHERE a.picking_id is not null
AND (b.date_done + interval '7 hour')::DATE >= ?::DATE
	AND (b.date_done + interval '7 hour')::DATE <= ?::DATE
	AND b.state = 'done'
	AND (c.id = ANY(string_to_array( ?, ',')::integer[]) or ? = '0')
	AND (b.id = ANY(string_to_array( ?, ',')::integer[]) or ? = '0')
	AND (e.id = ANY(string_to_array( ?, ',')::integer[]) or ? = '0')
	)
	
	UNION ALL

	(-- Trả hàng trên hồ sơ trị liệu
	SELECT 
	e.x_old_code as ma_kh
	,e.name as ten_kh
	,g.default_code as ma_sp
	,g.name as ten_sp
	, h.name as don_vi
	, d.product_uom_qty as so_luong
	, c.name as ma_hoa_don
	, c.date_done + interval '7 hour' as ngay_hoa_don
	, 'Trả hàng trên hồ sơ trị liệu' as nguon
	, j.name as chi_tiet_nguon
	, CASE WHEN i.code = 'outgoing' THEN 'Xuất' WHEN i.code = 'incoming' THEN 'Nhập' ELSE i.code END as loai
	FROM therapy_prescription_return_product_line a 
	LEFT JOIN therapy_prescription_return_product b ON a.therapy_prescription_return_product_id = b.id
	LEFT JOIN stock_picking c ON b.picking_id = c.id
	LEFT JOIN stock_move d ON c.id = d.picking_id
	LEFT JOIN res_partner e ON c.partner_id = e.id
	LEFT JOIN product_product f ON d.product_id = f.id
	LEFT JOIN product_template g ON f.product_tmpl_id = g.id
	LEFT JOIN product_uom h ON h.id = g.uom_id
	LEFT JOIN stock_picking_type i ON c.picking_type_id = i.id
	LEFT JOIN therapy_record j ON b.therapy_record_id = j.id
	WHERE b.state = 'done'
	AND (c.date_done + interval '7 hour')::DATE >= ?::DATE
	AND (c.date_done + interval '7 hour')::DATE <= ?::DATE
	AND c.state = 'done'
	AND (e.id = ANY(string_to_array( ?, ',')::integer[]) or ? = '0')
	AND (c.id = ANY(string_to_array( ?, ',')::integer[]) or ? = '0')
	AND (f.id = ANY(string_to_array( ?, ',')::integer[]) or ? = '0')
	)
) as chi_tiet_xuat_nhap_hang_theo_khach_hang]]></xml-property>
