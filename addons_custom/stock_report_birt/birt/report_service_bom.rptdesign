            <xml-property name="queryText"><![CDATA[SELECT 
c.default_code as ma_dv
, c.name as ten_dv
, e.default_code as ma_sp
, e.name as ten_sp
, a.product_qty as so_luong
, f.name as don_vi_tinh
FROM service_bom_line a
LEFT JOIN service_bom b ON a.bom_id = b.id
LEFT JOIN product_template c ON b.product_tmpl_id = c.id
LEFT JOIN product_product d ON d.id = a.product_id
LEFT JOIN product_template e ON d.product_tmpl_id = e.id 
LEFT JOIN product_uom f ON f.id = a.product_uom_id
ORDER BY c.default_code, e.default_code]]></xml-property>
