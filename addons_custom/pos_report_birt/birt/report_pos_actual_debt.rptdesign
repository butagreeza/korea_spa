            <xml-property name="queryText"><![CDATA[select o.date_order + interval '7' hour as ngay_don_hang, p.x_old_code as ma_kh, p.x_code as ma_kh_moi,p.name as ten_kh, p.phone as sdt_kh, o.name as ten_don_hang
, (select p.name from res_partner p,res_users u where u.partner_id=p.id and u.id=o.user_id) chuyen_gia_tu_van
, t.gia_tri_don_hang
,t.gia_tri_da_su_dung
,t.gia_tri_da_thanh_toan
,t.gia_tri_toi_da
,t.cong_no_thuc_te as cong_no_thuc_te_tren_db
,t.gia_tri_da_su_dung - t.gia_tri_da_thanh_toan as cong_no_thuc_te
, COALESCE((select residual from account_invoice i where t.order_id = i.x_pos_order_id ),0) cong_no_don_hang
, COALESCE((select STRING_AGG(distinct bb.name, ', ')
		FROM pos_revenue_allocation_line aa,hr_employee bb,pos_revenue_allocation cc
		where aa.employee_id = bb.id and aa.revenue_allocation_id = cc.id
		and cc.state = 'close' AND aa.order_id = t.order_id), '') as nguoi_huong_doanh_thu
from (
		select detail.order_id
			,sum(gia_tri_don_hang) gia_tri_don_hang
			, sum(gia_tri_da_thanh_toan) gia_tri_da_thanh_toan
			, sum(gia_tri_da_su_dung) gia_tri_da_su_dung
			,sum(gia_tri_toi_da) gia_tri_toi_da
			, sum(cong_no_thuc_te) cong_no_thuc_te
		from
				(
				select t.product_id
					,t.order_id
					,t.order_line_id
					,t.price_unit
					, COALESCE(SUM((COALESCE(l.price_unit, 0)*COALESCE(l.qty, 0))-(COALESCE(l.price_unit, 0)*COALESCE(l.qty, 0)/100*COALESCE(l.discount,0))-COALESCE(l.x_discount, 0)), 0) gia_tri_don_hang
					, COALESCE((SELECT SUM(COALESCE(aa.amount, 0)) FROM pos_payment_allocation_line aa, pos_payment_allocation bb
								where aa.payment_allocation_id = bb.id and bb.state = 'done' AND aa.order_id = t.order_id and aa.order_line_id=t.order_line_id
								AND aa.payment_allocation_id is not null), 0) as gia_tri_da_thanh_toan
					, COALESCE(SUM(t.qty_used*t.price_unit), 0) as gia_tri_da_su_dung
					, COALESCE(SUM(t.qty_max*t.price_unit), 0) gia_tri_toi_da
					, COALESCE(SUM(t.actual_debt), 0) cong_no_thuc_te
-- 					, COALESCE(SUM(t.actual_debt), 0) cong_no_thuc_te1
					,'therapy_record_product' source
				from therapy_record_product t
					,(select id,order_id, product_id,qty,price_unit,discount,x_discount from pos_order_line where price_unit<>0 and qty<>0 and discount<>100) l
				where order_line_id is not null and t.price_unit<>0
				and t.order_line_id=l.id
				group by t.product_id,t.order_id,t.order_line_id,t.price_unit
				
				
				UNION ALL
				
				
				select c.product_id,l.order_id,l.id order_line_id,l.price_unit
				, COALESCE(c.amount_total,0) as gia_tri_don_hang
				, COALESCE((SELECT SUM(COALESCE(aa.amount, 0)) FROM pos_payment_allocation_line aa, pos_payment_allocation bb
				where aa.payment_allocation_id = bb.id and bb.state = 'done' AND aa.order_id = l.order_id and aa.order_line_id=l.id
				AND aa.payment_allocation_id is not null), 0) as gia_tri_da_thanh_toan
				, (COALESCE(c.amount_total,0)-COALESCE(c.remain_amount, 0)) as gia_tri_da_su_dung
				, COALESCE(c.amount_total,0) as gia_tri_toi_da
				, ((COALESCE(c.amount_total,0)-COALESCE(c.remain_amount, 0)) - COALESCE((SELECT SUM(COALESCE(aa.amount, 0)) FROM pos_payment_allocation_line aa, pos_payment_allocation bb
				where aa.payment_allocation_id = bb.id and bb.state = 'done' AND aa.order_id = l.order_id and aa.order_line_id=l.id
				AND aa.payment_allocation_id is not null), 0)
				) cong_no_thuc_te
				,'izi_service_card_detail' source
				from izi_service_card_detail c, stock_production_lot s,
				(select id,order_id, product_id,price_unit from pos_order_line where price_unit<>0 and qty<>0 and discount<>100) l
				where c.lot_id=s."id"
				and s.x_order_id=l.order_id
				and c.product_id=l.product_id
				
				UNION ALL
				
				
				select p.product_id
					,p.order_id
					,p.order_line_id
					,p.price_unit 
					, p.gia_tri_don_hang
					, p.gia_tri_da_thanh_toan
					, p.gia_tri_da_su_dung
					,p.gia_tri_don_hang gia_tri_toi_da
					, (p.gia_tri_da_su_dung-p.gia_tri_da_thanh_toan) cong_no_thuc_te
					,'stock_move' source
				from (select l.product_id,l.order_id,l.id order_line_id,l.price_unit
				, COALESCE(SUM((COALESCE(l.price_unit, 0)*COALESCE(l.qty, 0))-(COALESCE(l.price_unit, 0)*COALESCE(l.qty, 0)/100*COALESCE(l.discount,0))-COALESCE(l.x_discount, 0)), 0) gia_tri_don_hang
				, COALESCE((SELECT SUM(COALESCE(aa.amount, 0)) FROM pos_payment_allocation_line aa, pos_payment_allocation bb
				where aa.payment_allocation_id = bb.id and bb.state = 'done' AND aa.order_id = l.order_id and aa.order_line_id=l.id
				AND aa.payment_allocation_id is not null), 0) as gia_tri_da_thanh_toan
				, COALESCE(SUM((COALESCE(l.price_unit, 0)*COALESCE(LEAST(m.qty,l.qty), 0))-(COALESCE(l.price_unit, 0)*COALESCE(LEAST(m.qty,l.qty), 0)/100*COALESCE(l.discount,0))-COALESCE(l.x_discount, 0)), 0) as gia_tri_da_su_dung
				from pos_order_line l,(select pos_order_id order_id,product_id,sum(product_qty) qty from pos_order_stock_picking_rel r, stock_move m
				where m.state = 'done' and r.stock_picking_id=m.picking_id
				group by order_id,product_id) m
				where l.order_id=m.order_id and l.product_id=m.product_id
				and l.price_unit<>0 and l.qty<>0 and l.discount<>100
				group by l.product_id,l.order_id,l.id,l.price_unit) p
		) detail
		group by detail.order_id
) 
t,pos_order o,res_partner p
WHERE t.order_id=o.id
and (o.date_order + interval '7')::DATE >= ?
and (o.date_order + interval '7')::DATE <= ?
and (p.id = ANY(string_to_array( ?, ',')::integer[]) or ? = '0')
and COALESCE((select residual from account_invoice i where t.order_id = i.x_pos_order_id ),0) > 0
and o.partner_id=p.id
and o.id not in (SELECT x_pos_partner_refund_id FROM pos_order WHERE x_pos_partner_refund_id is not null) --giao dịch bị refund
and o.x_pos_partner_refund_id is null --giao dịch refund
