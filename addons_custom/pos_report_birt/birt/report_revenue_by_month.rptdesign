            <xml-property name="queryText"><![CDATA[SELECT
CAST(abc.ngay as VARCHAR)
, abc.hinh_thuc
, abc.doanh_thu
FROM (
SELECT doanh_thu_theo_ngay.ngay
, SUM(doanh_thu_theo_ngay.doanh_thu) as doanh_thu
, doanh_thu_theo_ngay.hinh_thuc
FROM
(SELECT
(a.create_date + interval '7 hour')::DATE as ngay
, a.amount as doanh_thu
, c.name as hinh_thuc
FROM
account_bank_statement_line a
LEFT JOIN res_partner b ON a.partner_id = b.id
LEFT JOIN account_journal c ON a.journal_id = c.id
WHERE (a.create_date + interval '7 hour')::DATE >= ?::DATE
AND (a.create_date + interval '7 hour')::DATE <= ?::DATE
) as doanh_thu_theo_ngay
GROUP BY doanh_thu_theo_ngay.ngay,doanh_thu_theo_ngay.hinh_thuc
ORDER BY doanh_thu_theo_ngay.ngay,doanh_thu_theo_ngay.hinh_thuc
) as abc]]></xml-property>
