<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="report_account_cash_form" model="ir.ui.view">
            <field name="name">Report Account Cash</field>
            <field name="model">report.account.cash</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <group>
                            <field name="from_date" required="1"/>
                            <field name="to_date" required="1"/>
                        </group>
                        <group>
                            <field name="type" widget="Selection"/>
                        </group>
                    </group>
                    <footer>
                        <button class="oe_highlight" name="create_report_account_cash" type="object" string="Print Report"/>
                        <button class="oe_highlight" name="create_report_account_cash_excel" type="object" string="Print Report Excel"/>
                    </footer>
                </form>
            </field>
        </record>
        <record id="report_account_cash_action" model="ir.actions.act_window">
            <field name="name">Report Account Cash</field>
            <field name="res_model">report.account.cash</field>
            <field name="view_mode">form</field>
            <field name="view_type">form</field>
            <field name="target">new</field>
        </record>
        <!--<menuitem id="pos_report" name="Pos Report" parent="point_of_sale.menu_point_root" sequence="11"/>-->
        <menuitem id="report_account_cash_menu" action="report_account_cash_action" name="Report Account Cash" parent="point_of_sale.menu_point_rep"/>
    </data>
</odoo>